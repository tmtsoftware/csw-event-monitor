    "antd": "^4.16.12",
    "electron-snowpack": "^0.12.0",
    "react": "^17.0.2",
    "react-css-theme-switcher": "^0.3.0",
    "react-dom": "^17.0.2",
    "universal-cookie": "^4.0.4",
    "wildcard-match": "^5.1.2"
  },
  "devDependencies": {
    "@snowpack/plugin-react-refresh": "^2.5.0",
