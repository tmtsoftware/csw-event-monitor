    "chai": "^4.3.4",
    "electron": "^13.1.9",
    "electron-builder": "^22.11.7",
    "gulp": "^4.0.2",
    "gulp-csso": "^4.0.1",
    "gulp-debug": "^4.0.0",
    "gulp-less": "^5.0.0",
    "gulp-postcss": "^9.0.0",
    "less-plugin-npm-import": "^2.1.0",
    "mocha": "^9.0.3",
    "npm-run-all": "^4.1.5",
    "snowpack": "^3.8.6",
